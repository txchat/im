COMPOSE_PROJECT_NAME=im_mock
HOST_ETH0_NAME=127.0.0.1

## ZOOKEEPER
# zookeeper版本号
ZOOKEEPER_VERSION=latest
# zookeeper容器名称
ZOOKEEPER_CONTAINER_NAME=${COMPOSE_PROJECT_NAME}_zookeeper
# zookeeper数据卷
ZOOKEEPER_DATA_VOLUME=${COMPOSE_PROJECT_NAME}_zookeeper_data

## KAFKA
# kafka版本号
KAFKA_VERSION=2.13-2.8.1
# kafka容器名称
KAFKA_CONTAINER_NAME=${COMPOSE_PROJECT_NAME}_kafka
# kafka数据卷
KAFKA_DATA_VOLUME=${COMPOSE_PROJECT_NAME}_kafka_data

## KAFKA_MANAGER
# kafka容器名称
KAFKA_MANAGER_CONTAINER_NAME=${COMPOSE_PROJECT_NAME}_kafka_manager

## COMET
# comet服务镜像
COMET_IMAGE=${PROJECT_VERSION}
# comet容器名称
COMET_CONTAINER_NAME=${COMPOSE_PROJECT_NAME}_comet
# comet配置卷
COMET_CONFIG_VOLUME=${COMPOSE_PROJECT_NAME}_comet_config

## LOGIC
# logic服务镜像
LOGIC_IMAGE=${PROJECT_VERSION}
# logic容器名称
LOGIC_CONTAINER_NAME=${COMPOSE_PROJECT_NAME}_logic
# logic配置卷
LOGIC_CONFIG_VOLUME=${COMPOSE_PROJECT_NAME}_logic_config

## ECHO
# echo服务镜像
ECHO_IMAGE=${PROJECT_VERSION}
# echo容器名称
ECHO_CONTAINER_NAME=${COMPOSE_PROJECT_NAME}_echo
# echo配置卷
ECHO_CONFIG_VOLUME=${COMPOSE_PROJECT_NAME}_echo_config

## REDIS
# redis版本号
REDIS_VERSION=6.2.1
# redis容器名称
REDIS_CONTAINER_NAME=${COMPOSE_PROJECT_NAME}_redis
# redis数据卷
REDIS_DATA_VOLUME=${COMPOSE_PROJECT_NAME}_redis_data
# redis配置卷
REDIS_CONFIG_VOLUME=${COMPOSE_PROJECT_NAME}_redis_config
# redis日志卷
REDIS_LOG_VOLUME=${COMPOSE_PROJECT_NAME}_redis_log
# redis端口号
REDIS_PUB_PORT=127.0.0.1:6379
# redis连接密码
REDIS_PASSWORD=123456

## ETCD
# etcd版本号
ETCD_VERSION=v3.5.4
# etcd容器名称
ETCD_CONTAINER_NAME=${COMPOSE_PROJECT_NAME}_etcd
# etcd数据卷
ETCD_DATA_VOLUME=${COMPOSE_PROJECT_NAME}_etcd_data
# etcd客户端连接端口号
ETCD_PUB_CLIENT_PORT=2379
# etcd节点连接端口号
ETCD_PUB_PEER_PORT=2380
# etcd节点名称
ETCD_NAME=infra1
# etcd初始化令牌
ETCD_TOKEN=token
